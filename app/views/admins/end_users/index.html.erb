<h1>エンドユーザー一覧</h1>
<table>
  <tr>
    <th>名前<br>(カナ)</th>
    <th>Eメール</th>
    <th>ステータス</th>
  </tr>
  <tr>
    <% @all_end_users.each do |e_u| %>
      <td><%= e_u.last_name + e_u.first_name %><br><%= e_u.last_name_kana + e_u.first_name_kana %></td>
      <td><%= e_u.email %></td>
      <% if e_u.deleted_at %>
        <td>退会済み</td>
      <% else %>
        <td>有効</td>
      <% end %>
    <% end %>
  </tr>
</table>
